{
  "execute_on": "lxc",
  "name": "Enable MariaDB Service",
  "description": "Add MariaDB to OpenRC default runlevel",
  "commands": [
    {
      "name": "Enable MariaDB",
      "command": "rc-update add mariadb default >&2 && echo '[{\"id\": \"mariadb_enabled\", \"value\": \"true\"}]'"
    }
  ]
}
