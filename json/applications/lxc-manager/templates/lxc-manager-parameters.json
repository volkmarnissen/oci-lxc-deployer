{
  "execute_on": "ve",
  "name": "Set Parameters",
  "description": "Set application-specific parameters for Lxc Manager",
  "parameters": [],
  "commands": [
    {
      "name": "set-properties",
      "properties": [
        {
          "id": "hostname",
          "value": "lxc-manager"
        },
        {
          "id": "oci_image",
          "value": "ghcr.io/modbus2mqtt/lxc-manager"
        },
        {
          "id": "uid",
          "value": "0"
        },
        {
          "id": "username",
          "value": "root"
        },
        {
          "id": "volumes",
          "value": "config=/config\nsecure/secure,700"
        }
      ]
    }
  ]
}